\begin_layout Section
Ecological dynamics
\end_layout

\begin_layout Standard
We then have
\begin_inset Formula 
\begin{align*}
\frac{\mathrm{d}I_{1}}{\mathrm{d}t} & =-\left(a_{1}+\sigma_{1}+\mu_{I1}\right)I_{1}+\frac{1}{T_{1}}O_{1}+\sigma_{2}I_{2}\\
 & +\frac{1}{2}\frac{1}{r_{1}+1}I_{1}a_{1}bS_{1}\left[T_{1},\tilde{T}_{1}\right]\left(1-d_{1}+d_{1}\rho_{1}\right)+\frac{1}{2}\frac{1}{r_{2}+1}I_{2}a_{2}bS_{2}\left[T_{2},\tilde{T}_{2}\right]d_{2}\rho_{1}\\
 & +\frac{1}{2}\frac{1}{r_{1}+1}\tilde{I}_{1}\tilde{a}_{1}bS_{1}\left[T_{1},\tilde{T}_{1}\right]\left(1-d_{1}+d_{1}\rho_{1}\right)+\frac{1}{4}\frac{1}{r_{2}+1}\tilde{I}_{2}\tilde{a}_{2}bS_{2}\left[T_{2},\tilde{T}_{2}\right]d_{2}\rho_{1}\\
\frac{\mathrm{d}O_{1}}{\mathrm{d}t} & =-\left(\frac{1}{T_{1}}+\mu_{O1}+\sigma_{1}\right)O_{1}
\end{align*}

\end_inset


